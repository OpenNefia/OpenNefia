<DefaultWindow xmlns="https://opennefia.io"
               Class="OpenNefia.Core.ViewVariables.ViewVariablesAddComponentWindow">
    <BoxContainer Orientation="Vertical" VerticalExpand="True">
        <LineEdit Name="SearchLineEdit" PlaceHolder="Search..." HorizontalExpand="True" />
        <ItemList Name="EntryItemList" VerticalExpand="True" HorizontalExpand="True" SelectMode="Single" />
        <BoxContainer Orientation="Horizontal" HorizontalExpand="True">
            <Button Name="AddButton" Text="Select" TextAlign="Center" HorizontalExpand="True"/>
            <CheckBox Name="ShowAllCheckBox" Text="Show All" HorizontalExpand="True"/>
        </BoxContainer>
    </BoxContainer>
</DefaultWindow>
