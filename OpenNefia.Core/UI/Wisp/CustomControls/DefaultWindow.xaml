<DefaultWindow xmlns="https://opennefia.io" MinWidth="100" MinHeight="50" Class="OpenNefia.Core.UI.Wisp.CustomControls.DefaultWindow">
    <PanelContainer/>
    <BoxContainer Orientation="Vertical" SeparationOverride="0">
        <PanelContainer Name="WindowHeader">
            <BoxContainer Orientation="Horizontal">
                <Label Margin="5 0 0 0" HorizontalExpand="true" Name="TitleLabel" ClipText="True"
                       Text="defaultwindow-placeholder-title" VAlign="Center" />
                <TextureButton Name="CloseButton" VerticalAlignment="Center" />
            </BoxContainer>
        </PanelContainer>
        <WispControl Name="ContentsContainer" Margin="10" RectClipContent="True" VerticalExpand="true" />
    </BoxContainer>
</DefaultWindow>
